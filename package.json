{
  "name": "test",
  "module": "src/main.ts",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run --hot src/main.ts\" \"bun run uno:dev\" \"bun run liveReload\"",
    "liveReload": "bun run src/lib/liveReload.ts",
    "start": "bun run uno && bun run src/main.ts",
    "db:push": "bunx drizzle-kit push:sqlite",
    "db:studio": "bunx drizzle-kit studio",
    "db:seed": "bun run src/model/store/seed.ts",
    "uno": "bunx unocss",
    "uno:dev": "bunx unocss --watch",
    "typecheck": "bunx --bun tsc"
  },
  "devDependencies": {
    "@kitajs/ts-html-plugin": "^1.0.1",
    "bun-types": "latest",
    "concurrently": "^8.2.1",
    "drizzle-kit": "^0.19.13",
    "typescript": "^5.2.2",
    "unocss": "^0.55.7"
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "0.0.9",
    "@elysiajs/cron": "^0.6.0",
    "@elysiajs/static": "^0.6.0",
    "@elysiajs/swagger": "^0.6.2",
    "@libsql/client": "0.3.5-pre.4",
    "@lucia-auth/adapter-sqlite": "^2.0.0",
    "@t3-oss/env-core": "^0.6.1",
    "drizzle-orm": "^0.28.6",
    "drizzle-typebox": "^0.1.1",
    "elysia": "link:elysia",
    "elysia-autoroutes": "^0.2.2",
    "lucia": "^2.6.0",
    "pino-pretty": "^10.2.0",
    "zod": "^3.22.2",
    "beth-jsx": "link:beth-jsx"
  }
}
